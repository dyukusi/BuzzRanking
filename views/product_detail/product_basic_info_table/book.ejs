<tr>
    <th>タイトル</th>
    <td><%= productModel.getProductName() %></td>
</tr>
<tr>
    <th>カテゴリ</th>
    <td>
        <a href="/ranking/<%= productTypeBundleName %>">
            <%= productModel.getBelongedProductBundleJaName() %>
        </a>
    </td>
</tr>
<tr>
    <th>発売日</th>
    <td><%= productModel.saleDateStr %></td>
</tr>
<tr>
    <th>著者</th>
    <td><%= productModel.author %></td>
</tr>
<tr>
    <th>出版社</th>
    <td><%= productModel.publisher %></td>
</tr>
<% if (productModel.size) { %>
<tr>
    <th>種類</th>
    <td><%= productModel.size %></td>
</tr>
<% } %>
<tr>
    <th>注目度測定<span class="no-wrap">ワード</span></th>
    <td><%= twitterAltSearchWordsStr %></td>
</tr>
