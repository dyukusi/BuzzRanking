<%
var productTypeBundleNames = __.keys(Const.PRODUCT_TYPE_BUNDLE_NAME_TO_ID);

var productTypeBundleIdToAltText = {
  [Const.PRODUCT_TYPE_BUNDLE_NAME_TO_ID['inn']]: '<span class="no-wrap">宿泊・旅行</span><span class="no-wrap">予約</span>',
};
%>

<!DOCTYPE html>
<link href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" rel="stylesheet"
      integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css"
      integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" crossorigin="anonymous">
<link href="/css/common.css" rel="stylesheet">
<link href="/css/index.css" rel="stylesheet">

<html>
<head>
    <title>【毎日更新】BuzzRanking 今バズってる話題の商品・サービスをランキング形式で紹介！</title>
    <meta name="viewport"
          content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no">
    <meta name="description" content="今ネットでバズっている本や漫画、小説、ゲームなどの商品や、宿泊・旅行予約などのWebサービスを話題性の高い順に毎日ランキング形式で紹介します。">

    <script data-ad-client="ca-pub-8140865713225785" async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
</head>
<body>

<%- include(appRoot + '/views/header.ejs') %>

<div class="container-fluid p-3">

    <div class="grid-container">
        <%
        __.each(productTypeBundleNames, productTypeBundleName => {
          var productTypeBundleId = Const.PRODUCT_TYPE_BUNDLE_NAME_TO_ID[productTypeBundleName];
          var dispHTML = productTypeBundleIdToAltText[productTypeBundleId] || Const.PRODUCT_TYPE_BUNDLE_ID_TO_JA_NAME_HASH[productTypeBundleId];
        %>

            <div class="grid">
                <a href="/ranking/<%= productTypeBundleName %>">
                    <img src="img/category/<%= productTypeBundleName %>.jpg">
                </a>
                <p><%- dispHTML %></p>
            </div>

        <% }); %>
    </div>

</div>


</div>

<%- include(appRoot + '/views/footer.ejs') %>

</body>
</html>

<script src="/js/compressed/index.min.js" type="text/javascript"></script>
